{
    "manifest_version": 2,
    "name": "Tab Finder",
    "description": "Search tab from All openned tabs.",
    "version": "1.0.0",
    "permissions": [
        "tabs"
    ],
    "background": {
        "page": "data/background.html"
    },
    "browser_action": {
        "default_icon": {
            "19": "Icon-19.png",
            "38": "Icon-38.png"
        },
        "default_title": "Tab Finder",
        "default_popup": "data/popup.html"
    },
    "icons": {
        "16": "Icon-16.png",
        "48": "Icon-48.png",
        "128": "Icon-128.png"
    }
}
